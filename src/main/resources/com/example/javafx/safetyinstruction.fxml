                                       <children>
                                          <SplitPane dividerPositions="0.34593724859211583" layoutX="67.0" layoutY="57.0" orientation="VERTICAL" prefHeight="308.0" prefWidth="617.0" AnchorPane.bottomAnchor="-71.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <items>
